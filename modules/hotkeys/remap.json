{
  "title": "remap",
  "rules": [
    {
      "description": "Focus Kitty with Capslock",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock"
          },
          "to": [
            {
              "shell_command": "~/bin/hs -A -c 'toggleKitty()'"
            }
          ]
        }
      ]
    }
  ]
}
